{
  "manifest_version": 2,
  "name": "Border",
  "description": "Border на всё что попало",
  "version": "0.0.3",
  "author": "@deeamtee",
  "browser_action": {
    "default_title": "Border-JS",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "popup.js"]
    }
  ]
}
